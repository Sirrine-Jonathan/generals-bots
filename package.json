{
  "name": "generals-bots",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "run-script-os",
    "watch": "run-script-os",
    "start:windows": "set LOG=FALSE && node server",
    "watch:windows": "set LOG=TRUE && nodemon server",
    "start:macos": "export LOG=FALSE && node server",
    "watch:macos": "export LOG=TRUE && nodemon server",
    "start:default": "export LOG=FALSE && node server",
    "watch:default": "export LOG=TRUE && nodemon server",
    "build": "cd client/ && yarn install && yarn build",
    "test": "cd ./server && node index.js 5500",
    "refresh": "yarn install & cd ./server && rm -rf ./node_modules && yarn install & cd ./client && rm -rf ./node_modules && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sirrine-Jonathan/generals-bots.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sirrine-Jonathan/generals-bots/issues"
  },
  "homepage": "https://github.com/Sirrine-Jonathan/generals-bots#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "run-script-os": "^1.1.6"
  }
}
