{
  "name": "generals-bots",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server",
    "watch": "run-script-os",
    "watch:macos": "nodemon server > logs/$(date +%Y%m%d-%H%M%S).txt",
    "watch:windows": "nodemon server > logs/%date:~10,4%%date:~7,2%%date:~4,2%_%time:~0,2%%time:~3,2%.txt",
    "watch:default": "nodemon server",
    "build": "cd client/ && yarn install && yarn build",
    "test": "cd ./server && node index.js 5500",
    "delog": "rm -rf ./logs/*",
    "refresh": "yarn install & cd ./server && rm -rf ./node_modules && yarn install & cd ./client && rm -rf ./node_modules && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sirrine-Jonathan/generals-bots.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sirrine-Jonathan/generals-bots/issues"
  },
  "homepage": "https://github.com/Sirrine-Jonathan/generals-bots#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "run-script-os": "^1.1.6"
  }
}
